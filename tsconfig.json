{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": false,
    "jsx": "react-native",
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es5",
    "noImplicitAny": false,
    "experimentalDecorators": true
  },
  "exclude": ["node_modules"]
}
